@{
    ViewData["Title"] = "Numbers";
}

@model int[]

<div class="flex flex-col gap-6 w-3/4 mx-auto mt-8">
    <p class="text-center">Here is are some numbers!</p>
    <div class="border-solid border-2 border-black w-1/5 p-5">
        @{
            foreach(int nbr in @Model)
            {
                <p>@nbr</p>
            }
        }
    </div>
</div>