@{
    ViewData["Title"] = "Users";
}

@model List<User>

<div class="flex flex-col gap-6 w-3/4 mx-auto mt-8">
    <p class="text-center">Here is are some Users!</p>
    <div class="border-solid border-2 border-black w-1/5 p-5">
        @{
            foreach(User usr in @Model)
            {
                <p>@usr.FirstName @usr.LastName</p>
            }
        }
    </div>
</div>